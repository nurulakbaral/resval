{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/foo/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nrwl/web:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/foo",
        "tsConfig": "packages/foo/tsconfig.lib.json",
        "project": "packages/foo/package.json",
        "entryFile": "packages/foo/src/index.ts",
        "external": ["react/jsx-runtime"],
        "rollupConfig": "rollup.config.js",
        "compiler": "babel",
        "format": ["esm", "cjs"],
        "assets": [
          {
            "glob": "packages/foo/README.md",
            "input": ".",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/foo/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/foo"],
      "options": {
        "jestConfig": "packages/foo/jest.config.ts",
        "passWithNoTests": true
      }
    }
  }
}
